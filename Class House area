class Home:
    def __init__(self, door, window, walls, roof, kitchen):
        self.door = door
        self.window = window
        self.walls = walls
        self.roof = roof
        self.kitchen = kitchen

class Complex(Home):
    def __init__(self, elevator):
        self.elevator = elevator
    
    def install_elevator(self):
        if self.elevator != True:
            return "Вызовите лифт"
        elif self.elevator == True:
            return "Лифт едет"

class Dacha(Home):
    def __init__(self, electrcity):
        self.electrcity = electrcity

    def off_electrcity(self):
        while self.electrcity !=True:
            print('Нет света, включите рубильник')
        else:
            print('Свет есть')

class Palace(Home):
    def __init__(self, flag):
        self.flag = flag

    def rise_flag(self):
        if self.flag != True:
            print("Флаг спушен")
        else:
            print('Флаг поднят')


compl = Complex()
dach = Dacha()
pal = Palace()


compl.off_electrcity()
dach.rise_flag()
pal.install_elevator()
